<map proto="1.4.2">
<name>Ether KoTH</name>
<version>1.0.0</version>
<objective>Reach 750 points before the opposing team!</objective>
<authors>
    <author uuid="f610e558-a3c8-44fb-8622-68cc49dbc18f"/> <!-- jcane -->
</authors>
<contributors>
    <contributor uuid="621b2aa8-4e5f-4ed8-bd36-0ae2f8952a06"/> <!-- Strangey -->
    <contributor uuid="ac3cf065-38b1-4422-9541-e0ce6c5e824b"/> <!-- Hizashy -->
</contributors>
<teams>
    <team id="blue" color="blue" max="5" max-overfill="5">Blue</team>
    <team id="red" color="dark red" max="5" max-overfill="5">Red</team>
</teams>
<kits>
    <kit id="conquest">
        <clear/>
        <item slot="0" unbreakable="true" material="stone sword"/>
        <item slot="1" unbreakable="true" material="bow"/>
        <item slot="2" material="golden apple"/>
        <item slot="8" amount="8" material="arrow"/>
        <helmet locked="true" unbreakable="true" team-color="true" material="leather helmet"/>
        <chestplate locked="true" unbreakable="true" team-color="true" material="leather chestplate"/>
        <leggings locked="true" unbreakable="true" material="chainmail leggings">
            <enchantment level="4">projectile_protection</enchantment>
        </leggings>
        <boots locked="true" unbreakable="true" material="iron boots">
            <enchantment>feather_falling</enchantment>
            <enchantment>fire_protection</enchantment>
        </boots>
        <effect duration="1s" amplifier="0">health boost</effect>
        <effect duration="1s" amplifier="10">regeneration</effect>
        <game-mode>adventure</game-mode>
    </kit>
    <kit id="spawn-kit" parent="conquest">
        <effect duration="4s" amplifier="255">resistance</effect>
    </kit>
</kits>
<spawns>
    <default>
        <region yaw="0">
            <cuboid min="0.5,33.5,-40.5" max="0.5,33.5,-40.5"/>
        </region>
    </default>
    <spawn team="blue" kit="spawn-kit">
        <region yaw="-90">
            <cuboid min="-33.5,18.5,3.5" max="-33.5,18.5,3.5"/>
        </region>
    </spawn>
    <spawn team="red" kit="spawn-kit">
        <region yaw="90">
            <cuboid min="34.5,18.5,3.5" max="34.5,18.5,3.5"/>
        </region>
    </spawn>
</spawns>
<regions>
    <union id="spawner-stuff">
        <cuboid id="mid-spawn-area" min="0.5,17.2,1.5" max="0.5,17.2,1.5"/>
        <cuboid id="mid-spawn-radius" min="-12,13,-1" max="12,24,13"/>
    </union>
    <union id="cap-region-mid">
        <cuboid min="4,8,8" max="-3,12,15"/>
    </union>
    <union id="cap-region-south">
        <cuboid min="4,17,27" max="-3,21,34"/>
    </union>
    <union id="cap-region-north">
        <cuboid min="4,14,-27" max="-3,18,-20"/>
    </union>
    <apply block="never"/>
</regions>
<king>
    <hills required="false" capture-time="5s" points="1" time-multiplier="0" show-progress="true" neutral-state="true" incremental="true" permanent="false">
        <hill name="South Hill" capture="cap-region-south" progress="cap-region-south">
            <captured>
                <cuboid min="4,37,26" max="-3,70,36"/>
            </captured>
        </hill>
        <hill name="Mid Hill" capture="cap-region-mid" progress="cap-region-mid">
            <captured>
                <cuboid min="4,37,7" max="-3,70,17"/>
            </captured>
        </hill>
        <hill name="North Hill" capture="cap-region-north" progress="cap-region-north">
            <captured>
                <cuboid min="4,37,-27" max="-3,70,-18"/>
            </captured>
        </hill>
    </hills>
</king>
<score>
    <limit>750</limit>
</score>
<spawners>
    <spawner id="gapple-spawner" spawn-region="mid-spawn-area" player-region="mid-spawn-radius" max-entities="9999" delay="6s">
        <item amount="1" material="golden apple"/>
    </spawner>
</spawners>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>iron boots</item>
</itemremove>
<itemkeep>
    <item>golden apple</item>
    <item>arrow</item>
</itemkeep>
<hunger>
    <depletion>off</depletion>
</hunger>
<respawn delay="4s" auto="true" blackout="true"/>
<kill-reward>
    <item material="golden apple"/>
    <item amount="4" material="arrow"/>
</kill-reward>
</map>